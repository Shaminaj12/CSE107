#include <stdio.h>

int main(void) {
  int n = 10;
  int i;
  int sum = 0;
  for(i=2;i<=n;i=i+2){
    sum = sum + i;
  }
  printf("%d\n", sum);
  sum = 0;
  i = 2;
  while(i<=n){
    sum = sum + i;
    i=i+2;
  }
  printf("%d\n", sum);
  sum = 0;
  i = 2;
  do{
    sum = sum + i;
    i=i+2;
  }while(i<=n);
  printf("%d\n", sum);
  return 0;
}
